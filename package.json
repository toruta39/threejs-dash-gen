{
  "name": "threejs-docset-gen",
  "version": "1.0.0",
  "description": "Generate Three.js documentation as Dash docsets using jsdom",
  "main": "app.js",
  "scripts": {
    "start": "node app.js && tar --exclude='.DS_Store' -cvzf threejs.tgz threejs.docset",
    "dev": "node app.js",
    "build": "npm run start"
  },
  "dependencies": {
    "connect": "^3.7.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "jsdom": "^23.0.0",
    "serve-static": "^2.2.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "threejs",
    "documentation",
    "dash",
    "docset",
    "jsdom",
    "generator"
  ],
  "author": "Joshua Zhang (@toruta39)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/toruta39/threejs-dash-gen"
  }
}
